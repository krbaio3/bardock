{
  "name": "bardock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build --prod",
    "clean": "rimraf dist && rimraf coverage",
    "precucumber": "webdriver-manager update",
    "cucumber": "protractor cucumber/cucumber.conf.js",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "travis:build": "ng build",
    "travis:ci": "ng build --prod --source-map",
    "travis:e2e": "ng e2e --protractor-config=./e2e/protractor-ci.conf.js",
    "travis:test": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage --source-map",
    "travis:webdriver-update": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/cucumber": "4.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cucumber": "4.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-reporters": "2.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "4.2.0",
    "protractor-html-reporter-2": "1.0.2",
    "rimraf": "2.6.2",
    "ts-node": "~5.0.1",
